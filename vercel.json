{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "framework": "other",
  "nodejs": "20",
  "env": {
    "SKIP_VITE_SETUP": "true"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
